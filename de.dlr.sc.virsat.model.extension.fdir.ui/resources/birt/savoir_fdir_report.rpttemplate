        <script-data-source name="virsat" id="334">
            <method name="open"><![CDATA[// imports
importPackage(Packages.java.lang);
importPackage(Packages.java.util);
importPackage(Packages.org.eclipse.core.runtime);

// load bundles
appBundle = Platform.getBundle("de.dlr.sc.virsat.apps");
modelBundle = Platform.getBundle("de.dlr.sc.virsat.model.edit");
fdirBundle = Platform.getBundle("de.dlr.sc.virsat.model.extension.fdir");

// load classes
modelAPIClass = appBundle.loadClass("de.dlr.sc.virsat.apps.api.external.ModelAPI");
beanClass = modelBundle.loadClass("de.dlr.sc.virsat.model.concept.types.structural.IBeanStructuralElementInstance");

// set the correct user dir
path = reportContext.getDesignHandle().getSystemId().toString();
// we get back a path of the format file:/myDrive:/.../projectName/apps/ cut off file:/ and apps/
path = path.substring(6, path.length() - 6);
System.setProperty("user.dir", path);

// extract project name
splitPath = path.split("/");
projectName = splitPath[splitPath.length - 1];

// setup model api
modelAPI = modelAPIClass.newInstance();

// get the appropriate root element and the child elements
configuration = params["configuration"];

trees = modelAPI.getRootSeis(beanClass);
rootElement = trees.get(0);
for (i = 0; i < trees.size(); ++i) {
	if (trees.get(i).getName().equals(configuration)) {
		rootElement = trees.get(i);
		break;
	}
}

elements = new ArrayList();
elements.add(rootElement);
elements.addAll(rootElement.getDeepChildren(beanClass));

// load FDIR data
fdirParametersClass = fdirBundle.loadClass("de.dlr.sc.virsat.model.extension.fdir.model.FDIRParameters");
fdirParameters = null;
for (i = 0; i < elements.size(); ++i) {
	fdirParameters = elements.get(i).getFirst(fdirParametersClass);
	if (fdirParameters != null) {
		break;
	}
}]]></method>
        </script-data-source>
