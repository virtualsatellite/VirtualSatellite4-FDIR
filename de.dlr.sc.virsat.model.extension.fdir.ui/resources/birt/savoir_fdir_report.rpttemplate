            <method name="open"><![CDATA[fmecaClass = fdirBundle.loadClass("de.dlr.sc.virsat.model.extension.fdir.model.FMECA");
count = 0;
wrapLength = 13;

fmecaEntries = new ArrayList();
for (i = 0; i < elements.size(); ++i) {
	fmeca = elements.get(i).getFirst(fmecaClass);
	if (fmeca != null) {
		fmecaEntries.addAll(fmeca.getEntries());
	}
}]]></method>
            <method name="fetch"><![CDATA[// utility functions
function wrap(longStr, width) { 
	length = longStr.length(); 
 	if (length <= width) {
 		return longStr; 
 	}
 	return longStr.substring(0, width) + "\n" + wrap(longStr.substring(width, length), width);
} 

if (count < fmecaEntries.size()) {
	entry = fmecaEntries.get(count);
	row["item"] = entry.getParent().getStructuralElementInstance().getFullQualifiedInstanceName();
	row["failure"] = wrap(entry.getFailure().getName(), wrapLength);
	if (entry.getFailureMode() != null) {
		row["failureMode"] = wrap(entry.getFailureModeLabel(), wrapLength);
	}
	if (entry.getFailureCause() != null) {
		row["cause"] = wrap(entry.getFailureCause().getName(), wrapLength);
	}
	row["sn"] = entry.getSeverityEnum();
	if (entry.getProbabilityBean().isSet()) {
		row["pn"] = entry.getProbabilityEnum();
	}
	row["cn"] = entry.getCriticality();
	
	compensation = "";
	for (i = 0; i < entry.getProposedRecovery().size(); ++i) {
		compensation += entry.getProposedRecovery().get(i).getValue();
		if (i < entry.getProposedRecovery().size()) {
			compensation += ", ";
		}
	}
	
	row["compensation"] = compensation;
	
	effects =  "";
	for (i = 0; i < entry.getFailureEffects().size(); ++i) {
		effects += entry.getFailureEffects().get(i).getName();
		if (i < entry.getFailureEffects().size() - 1) {
			effects += ", ";
		}
	}
	
	row["failureEffects"] = effects;
	
	if (entry.getCriticality() != 0)  {
		row["isCritical"] = fdirParameters.isCritical(1, entry.getSeverityEnum(), entry.getProbabilityEnum());
	}
	
	count++;
	return true;
}

return false;]]></method>
