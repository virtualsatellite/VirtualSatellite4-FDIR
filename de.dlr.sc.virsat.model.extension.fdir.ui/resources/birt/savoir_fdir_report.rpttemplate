            <method name="fetch"><![CDATA[if (count < elements.size()) {
	element = elements.get(count++);
	row["no"] = count;
	
	// Create some +++ symbols to indicate the level of nesting
	prefix = "";
	parent = element.getParentSeiBean();
	while (parent != null) {
		prefix += "-";
		parent = parent.getParentSeiBean();
	}
	
	if (prefix != "") {
		prefix += " ";
	}
	
	row["name"] = prefix + element.getName();
	row["responsible"] = element.getStructuralElementInstance().getAssignedDiscipline().getName();
			
	return true;
}

return false;]]></method>
