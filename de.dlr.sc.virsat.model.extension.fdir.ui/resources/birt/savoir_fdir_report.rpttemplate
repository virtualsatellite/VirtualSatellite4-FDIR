            <property name="dataSource">virsat</property>
            <method name="open"><![CDATA[cutSetClass = fdirBundle.loadClass("de.dlr.sc.virsat.model.extension.fdir.model.CutSet");
fmecaClass = fdirBundle.loadClass("de.dlr.sc.virsat.model.extension.fdir.model.FMECAEntry");

pnNames = fmecaClass.getField("PL_NAMES").get(null);

countSn = 4;
snNames = fmecaClass.getField("SL_NAMES").get(null);

countDn = 1;
dnNames = cutSetClass.getField("DL_NAMES").get(null);]]></method>
            <method name="fetch"><![CDATA[if (countDn > dnNames.length) {
	return false;
}

row["sn"] = countSn;
row["dn"] = countDn;

row["severity"] = snNames[snNames.length - countSn];
row["detectability"] = dnNames[dnNames.length - countDn];

for (countPn = 1; countPn <= pnNames.length; ++countPn) {
	cn = countPn * countSn * countDn;
	row["cn_" + countPn] = cn;
	
	isCritical = fdirParameters.isCritical(countDn, countSn, countPn);
	row["isCritical_" + countPn] = isCritical;
}

if (countSn > 1) {
	countSn--;
} else {
	countDn++;
	countSn = snNames.length;
} 

return true;


]]></method>
